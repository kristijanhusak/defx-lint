Execute(Queue):
  let g:project_lint_test_queue = project_lint#queue#new()
Then:
  AssertEqual 1, g:project_lint_test_queue.is_empty()

Execute(Add to Queue):
  call g:project_lint_test_queue.add(1, { 'linter': { 'name': 'vim' } })
Then:
  AssertEqual 0, g:project_lint_test_queue.is_empty()
  AssertEqual ['vim'], g:project_lint_test_queue.get_running_linters()

Execute(Remove from Queue):
  call g:project_lint_test_queue.remove(1)
Then:
  AssertEqual 1, g:project_lint_test_queue.is_empty()
  AssertEqual [], g:project_lint_test_queue.get_running_linters()
